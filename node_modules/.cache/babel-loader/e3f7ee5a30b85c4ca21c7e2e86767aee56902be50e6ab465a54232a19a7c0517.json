{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n\nexport default {\n  name: 'LoginView',\n  data() {\n    return {\n      username: '',\n      password: '',\n      loading: false,\n      error: '',\n      showRegisterForm: false,\n      regUsername: '',\n      regPassword: '',\n      regLoading: false,\n      regError: '',\n      regSuccess: ''\n    };\n  },\n  created() {\n    // 如果已登录则跳转到文件列表页\n    if (localStorage.getItem('userId')) {\n      this.$router.push('/files');\n    }\n  },\n  methods: {\n    async login() {\n      this.loading = true;\n      this.error = '';\n      try {\n        const response = await this.$axios.post('/user/login', {\n          username: this.username,\n          password: this.password\n        });\n        if (response.data.success) {\n          localStorage.setItem('userId', response.data.userId);\n          localStorage.setItem('username', this.username);\n          this.$router.push('/files');\n        } else {\n          this.error = '登录失败，请检查用户名和密码';\n        }\n      } catch (error) {\n        console.error('登录错误:', error);\n        this.error = '登录失败，请稍后再试';\n      } finally {\n        this.loading = false;\n      }\n    },\n    async register() {\n      this.regLoading = true;\n      this.regError = '';\n      this.regSuccess = '';\n      try {\n        const response = await this.$axios.post('/user/register', {\n          username: this.regUsername,\n          password: this.regPassword\n        });\n        if (response.data.success) {\n          // 注册成功后直接登录\n          localStorage.setItem('userId', response.data.userId);\n          localStorage.setItem('username', this.regUsername);\n\n          // 显示成功消息并延迟跳转，给用户看到成功提示的时间\n          this.regSuccess = '注册成功，正在为您自动登录...';\n\n          // 延迟1秒后跳转到文件列表页\n          setTimeout(() => {\n            this.$router.push('/files');\n          }, 1000);\n        } else {\n          this.regError = '注册失败，可能用户名已存在';\n        }\n      } catch (error) {\n        console.error('注册错误:', error);\n        this.regError = '注册失败，请稍后再试';\n      } finally {\n        this.regLoading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","username","password","loading","error","showRegisterForm","regUsername","regPassword","regLoading","regError","regSuccess","created","localStorage","getItem","$router","push","methods","login","response","$axios","post","success","setItem","userId","console","register","setTimeout"],"sources":["src/components/Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <div class=\"login-card card\">\r\n      <h2>文件管理系统登录</h2>\r\n      <form @submit.prevent=\"login\">\r\n        <div class=\"form-group\">\r\n          <label for=\"username\">用户名</label>\r\n          <input \r\n            id=\"username\" \r\n            v-model=\"username\" \r\n            type=\"text\" \r\n            placeholder=\"请输入用户名\" \r\n            required\r\n          />\r\n        </div>\r\n        \r\n        <div class=\"form-group\">\r\n          <label for=\"password\">密码</label>\r\n          <input \r\n            id=\"password\" \r\n            v-model=\"password\" \r\n            type=\"password\" \r\n            placeholder=\"请输入密码\" \r\n            required\r\n          />\r\n        </div>\r\n        \r\n        <div class=\"error\" v-if=\"error\">{{ error }}</div>\r\n        \r\n        <button type=\"submit\" class=\"login-btn\" :disabled=\"loading\">\r\n          {{ loading ? '登录中...' : '登录' }}\r\n        </button>\r\n        \r\n        <div class=\"register-link\">\r\n          <p>没有账号？</p>\r\n          <button type=\"button\" @click=\"showRegisterForm = !showRegisterForm\">\r\n            {{ showRegisterForm ? '返回登录' : '注册账号' }}\r\n          </button>\r\n        </div>\r\n      </form>\r\n      \r\n      <!-- 注册表单 -->\r\n      <form v-if=\"showRegisterForm\" @submit.prevent=\"register\" class=\"register-form\">\r\n        <h3>创建新账号</h3>\r\n        <div class=\"form-group\">\r\n          <label for=\"reg-username\">用户名</label>\r\n          <input \r\n            id=\"reg-username\" \r\n            v-model=\"regUsername\" \r\n            type=\"text\" \r\n            placeholder=\"请输入用户名\" \r\n            required\r\n          />\r\n        </div>\r\n        \r\n        <div class=\"form-group\">\r\n          <label for=\"reg-password\">密码</label>\r\n          <input \r\n            id=\"reg-password\" \r\n            v-model=\"regPassword\" \r\n            type=\"password\" \r\n            placeholder=\"请输入密码\" \r\n            required\r\n          />\r\n        </div>\r\n        \r\n        <div class=\"error\" v-if=\"regError\">{{ regError }}</div>\r\n        <div class=\"success\" v-if=\"regSuccess\">{{ regSuccess }}</div>\r\n        \r\n        <button type=\"submit\" class=\"register-btn\" :disabled=\"regLoading\">\r\n          {{ regLoading ? '注册中...' : '注册' }}\r\n        </button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'LoginView',\r\n  data() {\r\n    return {\r\n      username: '',\r\n      password: '',\r\n      loading: false,\r\n      error: '',\r\n      \r\n      showRegisterForm: false,\r\n      regUsername: '',\r\n      regPassword: '',\r\n      regLoading: false,\r\n      regError: '',\r\n      regSuccess: ''\r\n    }\r\n  },\r\n  created() {\r\n    // 如果已登录则跳转到文件列表页\r\n    if (localStorage.getItem('userId')) {\r\n      this.$router.push('/files');\r\n    }\r\n  },\r\n  methods: {\r\n    async login() {\r\n      this.loading = true;\r\n      this.error = '';\r\n      \r\n      try {\r\n        const response = await this.$axios.post('/user/login', {\r\n          username: this.username,\r\n          password: this.password\r\n        });\r\n        \r\n        if (response.data.success) {\r\n          localStorage.setItem('userId', response.data.userId);\r\n          localStorage.setItem('username', this.username);\r\n          this.$router.push('/files');\r\n        } else {\r\n          this.error = '登录失败，请检查用户名和密码';\r\n        }\r\n      } catch (error) {\r\n        console.error('登录错误:', error);\r\n        this.error = '登录失败，请稍后再试';\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n    \r\n    async register() {\r\n      this.regLoading = true;\r\n      this.regError = '';\r\n      this.regSuccess = '';\r\n      \r\n      try {\r\n        const response = await this.$axios.post('/user/register', {\r\n          username: this.regUsername,\r\n          password: this.regPassword\r\n        });\r\n        \r\n        if (response.data.success) {\r\n          // 注册成功后直接登录\r\n          localStorage.setItem('userId', response.data.userId);\r\n          localStorage.setItem('username', this.regUsername);\r\n          \r\n          // 显示成功消息并延迟跳转，给用户看到成功提示的时间\r\n          this.regSuccess = '注册成功，正在为您自动登录...';\r\n          \r\n          // 延迟1秒后跳转到文件列表页\r\n          setTimeout(() => {\r\n            this.$router.push('/files');\r\n          }, 1000);\r\n          \r\n        } else {\r\n          this.regError = '注册失败，可能用户名已存在';\r\n        }\r\n      } catch (error) {\r\n        console.error('注册错误:', error);\r\n        this.regError = '注册失败，请稍后再试';\r\n      } finally {\r\n        this.regLoading = false;\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  min-height: 80vh;\r\n}\r\n\r\n.login-card {\r\n  width: 100%;\r\n  max-width: 400px;\r\n}\r\n\r\n.login-card h2, .login-card h3 {\r\n  margin-bottom: 1.5rem;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n}\r\n\r\n.login-btn, .register-btn {\r\n  width: 100%;\r\n  margin-top: 1rem;\r\n  padding: 0.75rem;\r\n}\r\n\r\n.register-link {\r\n  margin-top: 1.5rem;\r\n  text-align: center;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  gap: 0.5rem;\r\n}\r\n\r\n.register-link button {\r\n  background: none;\r\n  color: #3498db;\r\n  border: none;\r\n  padding: 0;\r\n  font: inherit;\r\n  cursor: pointer;\r\n  text-decoration: underline;\r\n}\r\n\r\n.register-link button:hover {\r\n  color: #2980b9;\r\n  background: none;\r\n}\r\n\r\n.register-form {\r\n  margin-top: 2rem;\r\n  border-top: 1px solid #eee;\r\n  padding-top: 1.5rem;\r\n}\r\n</style> "],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8EA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;MACAC,QAAA;MACAC,OAAA;MACAC,KAAA;MAEAC,gBAAA;MACAC,WAAA;MACAC,WAAA;MACAC,UAAA;MACAC,QAAA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACA;IACA,IAAAC,YAAA,CAAAC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,MAAA;MACA,KAAAd,OAAA;MACA,KAAAC,KAAA;MAEA;QACA,MAAAc,QAAA,cAAAC,MAAA,CAAAC,IAAA;UACAnB,QAAA,OAAAA,QAAA;UACAC,QAAA,OAAAA;QACA;QAEA,IAAAgB,QAAA,CAAAlB,IAAA,CAAAqB,OAAA;UACAT,YAAA,CAAAU,OAAA,WAAAJ,QAAA,CAAAlB,IAAA,CAAAuB,MAAA;UACAX,YAAA,CAAAU,OAAA,kBAAArB,QAAA;UACA,KAAAa,OAAA,CAAAC,IAAA;QACA;UACA,KAAAX,KAAA;QACA;MACA,SAAAA,KAAA;QACAoB,OAAA,CAAApB,KAAA,UAAAA,KAAA;QACA,KAAAA,KAAA;MACA;QACA,KAAAD,OAAA;MACA;IACA;IAEA,MAAAsB,SAAA;MACA,KAAAjB,UAAA;MACA,KAAAC,QAAA;MACA,KAAAC,UAAA;MAEA;QACA,MAAAQ,QAAA,cAAAC,MAAA,CAAAC,IAAA;UACAnB,QAAA,OAAAK,WAAA;UACAJ,QAAA,OAAAK;QACA;QAEA,IAAAW,QAAA,CAAAlB,IAAA,CAAAqB,OAAA;UACA;UACAT,YAAA,CAAAU,OAAA,WAAAJ,QAAA,CAAAlB,IAAA,CAAAuB,MAAA;UACAX,YAAA,CAAAU,OAAA,kBAAAhB,WAAA;;UAEA;UACA,KAAAI,UAAA;;UAEA;UACAgB,UAAA;YACA,KAAAZ,OAAA,CAAAC,IAAA;UACA;QAEA;UACA,KAAAN,QAAA;QACA;MACA,SAAAL,KAAA;QACAoB,OAAA,CAAApB,KAAA,UAAAA,KAAA;QACA,KAAAK,QAAA;MACA;QACA,KAAAD,UAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}