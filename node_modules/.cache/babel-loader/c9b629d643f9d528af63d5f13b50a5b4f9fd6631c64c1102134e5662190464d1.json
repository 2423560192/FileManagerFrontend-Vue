{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n\nexport default {\n  name: 'FileDetail',\n  props: {\n    id: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      loading: true,\n      error: '',\n      file: {},\n      userId: localStorage.getItem('userId'),\n      // ÁºñËæëÊñá‰ª∂Âêç\n      editFilename: false,\n      newFilename: '',\n      updateFilenameLoading: false,\n      // ÁºñËæëÊùÉÈôê\n      editPermission: false,\n      newPermission: '',\n      updatePermissionLoading: false,\n      // ÁºñËæëÂÜÖÂÆπ\n      editContent: false,\n      newContent: '',\n      updateContentLoading: false\n    };\n  },\n  computed: {\n    formattedContent() {\n      if (!this.file.content) return [''];\n\n      // ÊØè50‰∏™Â≠óÁ¨¶ÂàÜÂâ≤Êàê‰∏ÄË°å\n      const content = this.file.content;\n      const lines = [];\n      for (let i = 0; i < content.length; i += 50) {\n        lines.push(content.substring(i, i + 50));\n      }\n      return lines;\n    },\n    canRead() {\n      // Âè™Êúâ 'read' Êàñ 'readwrite' ÊùÉÈôêÊâçËÉΩËØªÂèñ\n      const permission = this.file.permission;\n      console.log('ÂΩìÂâçÊñá‰ª∂ÊùÉÈôê:', permission); // Ê∑ªÂä†Ë∞ÉËØïÊó•Âøó\n      return permission === 'read' || permission === 'readwrite';\n    },\n    canWrite() {\n      // Âè™Êúâ 'write' Êàñ 'readwrite' ÊùÉÈôêÊâçËÉΩÂÜôÂÖ•\n      const permission = this.file.permission;\n      console.log('ÂΩìÂâçÊñá‰ª∂ÊùÉÈôê:', permission); // Ê∑ªÂä†Ë∞ÉËØïÊó•Âøó\n      return permission === 'write' || permission === 'readwrite';\n    }\n  },\n  created() {\n    this.fetchFileDetails();\n  },\n  methods: {\n    async fetchFileDetails() {\n      this.loading = true;\n      try {\n        const response = await this.$axios.get(`/file/${this.id}`);\n        if (response.data.success) {\n          this.file = response.data.file;\n          this.newFilename = this.file.filename;\n          this.newPermission = this.file.permission;\n          this.newContent = this.file.content;\n        } else {\n          // ÊòæÁ§∫ÂêéÁ´ØËøîÂõûÁöÑÈîôËØØ‰ø°ÊÅØ\n          this.error = response.data.message;\n          this.file = {}; // Ê∏ÖÁ©∫Êñá‰ª∂Êï∞ÊçÆ\n        }\n      } catch (error) {\n        console.error('Ëé∑ÂèñÊñá‰ª∂ËØ¶ÊÉÖÂ§±Ë¥•:', error);\n        // Â¶ÇÊûúÊòØÂêéÁ´ØËøîÂõûÁöÑÈîôËØØ‰ø°ÊÅØÔºåÂàôÊòæÁ§∫ËØ•‰ø°ÊÅØ\n        if (error.response && error.response.data) {\n          this.error = error.response.data.message;\n        } else {\n          this.error = 'Ëé∑ÂèñÊñá‰ª∂ËØ¶ÊÉÖÂ§±Ë¥•ÔºåÂèØËÉΩÊñá‰ª∂‰∏çÂ≠òÂú®ÊàñÂ∑≤Ë¢´Âà†Èô§';\n        }\n      } finally {\n        this.loading = false;\n      }\n    },\n    goBack() {\n      this.$router.push('/files');\n    },\n    // Êñá‰ª∂ÂêçÁºñËæë\n    cancelFilenameEdit() {\n      this.editFilename = false;\n      this.newFilename = this.file.filename;\n    },\n    async updateFilename() {\n      if (!this.newFilename || this.newFilename === this.file.filename) {\n        this.editFilename = false;\n        return;\n      }\n      this.updateFilenameLoading = true;\n      try {\n        const response = await this.$axios.put(`/file/name/${this.id}`, {\n          filename: this.newFilename\n        });\n        if (response.data.success) {\n          this.file = response.data.file; // ‰ΩøÁî®ËøîÂõûÁöÑ file Êï∞ÊçÆ\n          this.editFilename = false;\n        } else {\n          throw new Error(response.data.message || 'Êõ¥Êñ∞Êñá‰ª∂ÂêçÂ§±Ë¥•');\n        }\n      } catch (error) {\n        console.error('Êõ¥Êñ∞Êñá‰ª∂ÂêçÂ§±Ë¥•:', error);\n        alert(error.message || 'Êõ¥Êñ∞Êñá‰ª∂ÂêçÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï');\n      } finally {\n        this.updateFilenameLoading = false;\n      }\n    },\n    // ÊùÉÈôêÁºñËæë\n    cancelPermissionEdit() {\n      this.editPermission = false;\n      this.newPermission = this.file.permission;\n    },\n    async updatePermission() {\n      if (this.newPermission === this.file.permission) {\n        this.editPermission = false;\n        return;\n      }\n      this.updatePermissionLoading = true;\n      try {\n        const response = await this.$axios.put(`/file/permission/${this.id}`, {\n          permission: this.newPermission\n        });\n        if (response.data.success) {\n          this.file = response.data.file; // ‰ΩøÁî®ËøîÂõûÁöÑ file Êï∞ÊçÆ\n          this.editPermission = false;\n        } else {\n          throw new Error(response.data.message || 'Êõ¥Êñ∞Êñá‰ª∂ÊùÉÈôêÂ§±Ë¥•');\n        }\n      } catch (error) {\n        console.error('Êõ¥Êñ∞Êñá‰ª∂ÊùÉÈôêÂ§±Ë¥•:', error);\n        alert(error.message || 'Êõ¥Êñ∞Êñá‰ª∂ÊùÉÈôêÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï');\n      } finally {\n        this.updatePermissionLoading = false;\n      }\n    },\n    // ÂÜÖÂÆπÁºñËæë\n    startEditContent() {\n      this.editContent = true;\n      this.newContent = this.file.content || '';\n    },\n    cancelContentEdit() {\n      this.editContent = false;\n      this.newContent = this.file.content;\n    },\n    async updateContent() {\n      this.updateContentLoading = true;\n      try {\n        const response = await this.$axios.put(`/file/content/${this.id}`, {\n          content: this.newContent\n        });\n        if (response.data.success) {\n          this.file = response.data.file; // ‰ΩøÁî®ËøîÂõûÁöÑ file Êï∞ÊçÆ\n          this.editContent = false;\n        } else {\n          throw new Error(response.data.message || 'Êõ¥Êñ∞Êñá‰ª∂ÂÜÖÂÆπÂ§±Ë¥•');\n        }\n      } catch (error) {\n        console.error('Êõ¥Êñ∞Êñá‰ª∂ÂÜÖÂÆπÂ§±Ë¥•:', error);\n        alert(error.message || 'Êõ¥Êñ∞Êñá‰ª∂ÂÜÖÂÆπÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï');\n      } finally {\n        this.updateContentLoading = false;\n      }\n    },\n    formatFileSize(size) {\n      const fileSize = size || this.file.fileSize;\n      if (!fileSize && fileSize !== 0) return 'Êú™Áü•';\n      if (fileSize < 1024) {\n        return `${fileSize} B`;\n      } else if (fileSize < 1024 * 1024) {\n        return `${(fileSize / 1024).toFixed(2)} KB`;\n      } else {\n        return `${(fileSize / (1024 * 1024)).toFixed(2)} MB`;\n      }\n    },\n    formatDate(timestamp) {\n      if (!timestamp) return 'Êú™Áü•';\n      const date = new Date(timestamp);\n      return date.toLocaleString('zh-CN');\n    },\n    getPermissionText(permission) {\n      const map = {\n        'read': 'Âè™ËØª',\n        'write': 'Âè™ÂÜô',\n        'readwrite': 'ËØªÂÜô'\n      };\n      return map[permission] || permission;\n    },\n    formatContent(content) {\n      // ÊØè50‰∏™Â≠óÁ¨¶Êç¢Ë°å\n      return content.replace(/(.{50})/g, '$1\\n');\n    }\n  }\n};","map":{"version":3,"names":["name","props","id","type","String","required","data","loading","error","file","userId","localStorage","getItem","editFilename","newFilename","updateFilenameLoading","editPermission","newPermission","updatePermissionLoading","editContent","newContent","updateContentLoading","computed","formattedContent","content","lines","i","length","push","substring","canRead","permission","console","log","canWrite","created","fetchFileDetails","methods","response","$axios","get","success","filename","message","goBack","$router","cancelFilenameEdit","updateFilename","put","Error","alert","cancelPermissionEdit","updatePermission","startEditContent","cancelContentEdit","updateContent","formatFileSize","size","fileSize","toFixed","formatDate","timestamp","date","Date","toLocaleString","getPermissionText","map","formatContent","replace"],"sources":["src/components/FileDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"file-detail\">\r\n    <div class=\"back-link\">\r\n      <button @click=\"goBack\" class=\"back-btn\">\r\n        <span class=\"back-icon\">‚Üê</span>\r\n        ËøîÂõûÊñá‰ª∂ÂàóË°®\r\n      </button>\r\n    </div>\r\n    \r\n    <div v-if=\"loading\" class=\"loading\">Âä†ËΩΩ‰∏≠...</div>\r\n    \r\n    <div v-else-if=\"error\" class=\"error-card card\">\r\n      <h3>Êó†Ê≥ïËÆøÈóÆÊñá‰ª∂</h3>\r\n      <p class=\"error-message\">{{ error }}</p>\r\n      <button @click=\"goBack\" class=\"back-btn\">ËøîÂõûÊñá‰ª∂ÂàóË°®</button>\r\n    </div>\r\n    \r\n    <div v-else class=\"content-card\">\r\n      <div class=\"file-header\">\r\n        <h2 class=\"file-title\">\r\n          <span class=\"file-icon\">üìÑ</span>\r\n          {{ file.filename }}\r\n        </h2>\r\n        <div class=\"file-meta\">\r\n          <span class=\"meta-item\">\r\n            <span class=\"meta-icon\">üìè</span>\r\n            Â§ßÂ∞èÔºö{{ formatFileSize(file.size) }}\r\n          </span>\r\n          <span class=\"meta-item\">\r\n            <span class=\"meta-icon\">üîí</span>\r\n            ÊùÉÈôêÔºö{{ getPermissionText(file.permission) }}\r\n          </span>\r\n          <span class=\"meta-item\">\r\n            <span class=\"meta-icon\">üïí</span>\r\n            ÂàõÂª∫Êó∂Èó¥Ôºö{{ formatDate(file.createTime) }}\r\n          </span>\r\n        </div>\r\n      </div>\r\n      \r\n      <div class=\"file-content\">\r\n        <h3>Êñá‰ª∂ÂÜÖÂÆπ</h3>\r\n        <div class=\"content-viewer\">\r\n          <pre>{{ formatContent(file.content) }}</pre>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'FileDetail',\r\n  props: {\r\n    id: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      error: '',\r\n      file: {},\r\n      userId: localStorage.getItem('userId'),\r\n      \r\n      // ÁºñËæëÊñá‰ª∂Âêç\r\n      editFilename: false,\r\n      newFilename: '',\r\n      updateFilenameLoading: false,\r\n      \r\n      // ÁºñËæëÊùÉÈôê\r\n      editPermission: false,\r\n      newPermission: '',\r\n      updatePermissionLoading: false,\r\n      \r\n      // ÁºñËæëÂÜÖÂÆπ\r\n      editContent: false,\r\n      newContent: '',\r\n      updateContentLoading: false\r\n    }\r\n  },\r\n  computed: {\r\n    formattedContent() {\r\n      if (!this.file.content) return [''];\r\n      \r\n      // ÊØè50‰∏™Â≠óÁ¨¶ÂàÜÂâ≤Êàê‰∏ÄË°å\r\n      const content = this.file.content;\r\n      const lines = [];\r\n      for (let i = 0; i < content.length; i += 50) {\r\n        lines.push(content.substring(i, i + 50));\r\n      }\r\n      return lines;\r\n    },\r\n    canRead() {\r\n      // Âè™Êúâ 'read' Êàñ 'readwrite' ÊùÉÈôêÊâçËÉΩËØªÂèñ\r\n      const permission = this.file.permission;\r\n      console.log('ÂΩìÂâçÊñá‰ª∂ÊùÉÈôê:', permission); // Ê∑ªÂä†Ë∞ÉËØïÊó•Âøó\r\n      return permission === 'read' || permission === 'readwrite';\r\n    },\r\n    canWrite() {\r\n      // Âè™Êúâ 'write' Êàñ 'readwrite' ÊùÉÈôêÊâçËÉΩÂÜôÂÖ•\r\n      const permission = this.file.permission;\r\n      console.log('ÂΩìÂâçÊñá‰ª∂ÊùÉÈôê:', permission); // Ê∑ªÂä†Ë∞ÉËØïÊó•Âøó\r\n      return permission === 'write' || permission === 'readwrite';\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchFileDetails();\r\n  },\r\n  methods: {\r\n    async fetchFileDetails() {\r\n      this.loading = true;\r\n      try {\r\n        const response = await this.$axios.get(`/file/${this.id}`);\r\n        \r\n        if (response.data.success) {\r\n          this.file = response.data.file;\r\n          this.newFilename = this.file.filename;\r\n          this.newPermission = this.file.permission;\r\n          this.newContent = this.file.content;\r\n        } else {\r\n          // ÊòæÁ§∫ÂêéÁ´ØËøîÂõûÁöÑÈîôËØØ‰ø°ÊÅØ\r\n          this.error = response.data.message;\r\n          this.file = {}; // Ê∏ÖÁ©∫Êñá‰ª∂Êï∞ÊçÆ\r\n        }\r\n      } catch (error) {\r\n        console.error('Ëé∑ÂèñÊñá‰ª∂ËØ¶ÊÉÖÂ§±Ë¥•:', error);\r\n        // Â¶ÇÊûúÊòØÂêéÁ´ØËøîÂõûÁöÑÈîôËØØ‰ø°ÊÅØÔºåÂàôÊòæÁ§∫ËØ•‰ø°ÊÅØ\r\n        if (error.response && error.response.data) {\r\n          this.error = error.response.data.message;\r\n        } else {\r\n          this.error = 'Ëé∑ÂèñÊñá‰ª∂ËØ¶ÊÉÖÂ§±Ë¥•ÔºåÂèØËÉΩÊñá‰ª∂‰∏çÂ≠òÂú®ÊàñÂ∑≤Ë¢´Âà†Èô§';\r\n        }\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n    \r\n    goBack() {\r\n      this.$router.push('/files');\r\n    },\r\n    \r\n    // Êñá‰ª∂ÂêçÁºñËæë\r\n    cancelFilenameEdit() {\r\n      this.editFilename = false;\r\n      this.newFilename = this.file.filename;\r\n    },\r\n    \r\n    async updateFilename() {\r\n      if (!this.newFilename || this.newFilename === this.file.filename) {\r\n        this.editFilename = false;\r\n        return;\r\n      }\r\n      \r\n      this.updateFilenameLoading = true;\r\n      try {\r\n        const response = await this.$axios.put(`/file/name/${this.id}`, {\r\n          filename: this.newFilename\r\n        });\r\n        if (response.data.success) {\r\n          this.file = response.data.file;  // ‰ΩøÁî®ËøîÂõûÁöÑ file Êï∞ÊçÆ\r\n          this.editFilename = false;\r\n        } else {\r\n          throw new Error(response.data.message || 'Êõ¥Êñ∞Êñá‰ª∂ÂêçÂ§±Ë¥•');\r\n        }\r\n      } catch (error) {\r\n        console.error('Êõ¥Êñ∞Êñá‰ª∂ÂêçÂ§±Ë¥•:', error);\r\n        alert(error.message || 'Êõ¥Êñ∞Êñá‰ª∂ÂêçÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï');\r\n      } finally {\r\n        this.updateFilenameLoading = false;\r\n      }\r\n    },\r\n    \r\n    // ÊùÉÈôêÁºñËæë\r\n    cancelPermissionEdit() {\r\n      this.editPermission = false;\r\n      this.newPermission = this.file.permission;\r\n    },\r\n    \r\n    async updatePermission() {\r\n      if (this.newPermission === this.file.permission) {\r\n        this.editPermission = false;\r\n        return;\r\n      }\r\n      \r\n      this.updatePermissionLoading = true;\r\n      try {\r\n        const response = await this.$axios.put(`/file/permission/${this.id}`, {\r\n          permission: this.newPermission\r\n        });\r\n        if (response.data.success) {\r\n          this.file = response.data.file;  // ‰ΩøÁî®ËøîÂõûÁöÑ file Êï∞ÊçÆ\r\n          this.editPermission = false;\r\n        } else {\r\n          throw new Error(response.data.message || 'Êõ¥Êñ∞Êñá‰ª∂ÊùÉÈôêÂ§±Ë¥•');\r\n 