{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n\nexport default {\n  name: 'FileList',\n  data() {\n    return {\n      loading: true,\n      files: [],\n      error: '',\n      userId: localStorage.getItem('userId'),\n      // Êñ∞Êñá‰ª∂Ë°®Âçï\n      newFile: {\n        filename: '',\n        content: '',\n        permission: 'read'\n      },\n      creating: false,\n      createError: '',\n      createSuccess: '',\n      // Âà†Èô§Á°ÆËÆ§\n      showDeleteConfirm: false,\n      selectedFile: null,\n      deleting: false,\n      // Ê∑ªÂä†ÁºñËæëÁõ∏ÂÖ≥ÁöÑÊï∞ÊçÆ\n      showEditModal: false,\n      editingFile: null,\n      editError: '',\n      updating: false,\n      // ËÆ∞ÂΩïÊâìÂºÄÁöÑÊñá‰ª∂IDÂàóË°®\n      openFiles: new Set(JSON.parse(localStorage.getItem('openFiles') || '[]')),\n      opening: null,\n      closing: null,\n      openedFile: null // ÂΩìÂâçÊâìÂºÄÁöÑÊñá‰ª∂\n    };\n  },\n  created() {\n    this.fetchFiles();\n  },\n  methods: {\n    async fetchFiles() {\n      this.loading = true;\n      this.error = '';\n      try {\n        const userId = localStorage.getItem('userId');\n        const response = await this.$axios.get(`/file/list/${userId}`);\n        if (response.data.success) {\n          // ‰∏∫ÊØè‰∏™Êñá‰ª∂Ê∑ªÂä†ÊâìÂºÄÁä∂ÊÄÅ\n          this.files = (response.data.files || []).map(file => ({\n            ...file,\n            isOpen: this.openFiles.has(file.id)\n          }));\n        } else {\n          this.error = 'Ëé∑ÂèñÊñá‰ª∂ÂàóË°®Â§±Ë¥•';\n        }\n      } catch (error) {\n        console.error('Ëé∑ÂèñÊñá‰ª∂ÂàóË°®Â§±Ë¥•:', error);\n        if (error.response) {\n          console.error('ÈîôËØØÂìçÂ∫î:', error.response.data);\n          this.error = error.response.data.message || 'Ëé∑ÂèñÊñá‰ª∂ÂàóË°®Â§±Ë¥•ÔºåÊúçÂä°Âô®ÈîôËØØ';\n        } else if (error.request) {\n          this.error = 'Êó†Ê≥ïËøûÊé•Âà∞ÊúçÂä°Âô®ÔºåËØ∑Ê£ÄÊü•ÁΩëÁªúËøûÊé•';\n        } else {\n          this.error = 'Ëé∑ÂèñÊñá‰ª∂ÂàóË°®Â§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï';\n        }\n      } finally {\n        this.loading = false;\n      }\n    },\n    async createFile() {\n      this.creating = true;\n      this.createError = '';\n      this.createSuccess = '';\n      try {\n        const userId = localStorage.getItem('userId');\n        await this.$axios.post('/file/create', {\n          userId: parseInt(userId),\n          filename: this.newFile.filename,\n          content: this.newFile.content || '',\n          permission: this.newFile.permission\n        });\n        this.createSuccess = 'Êñá‰ª∂ÂàõÂª∫ÊàêÂäüÔºÅ';\n        // ÈáçÁΩÆË°®Âçï\n        this.newFile = {\n          filename: '',\n          content: '',\n          permission: 'read'\n        };\n        // Âà∑Êñ∞Êñá‰ª∂ÂàóË°®\n        await this.fetchFiles();\n      } catch (error) {\n        console.error('ÂàõÂª∫Êñá‰ª∂Â§±Ë¥•:', error);\n        if (error.response) {\n          this.createError = error.response.data.message || 'ÂàõÂª∫Êñá‰ª∂Â§±Ë¥•ÔºåÊúçÂä°Âô®ÈîôËØØ';\n        } else if (error.request) {\n          this.createError = 'Êó†Ê≥ïËøûÊé•Âà∞ÊúçÂä°Âô®ÔºåËØ∑Ê£ÄÊü•ÁΩëÁªúËøûÊé•';\n        } else {\n          this.createError = 'ÂàõÂª∫Êñá‰ª∂Â§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï';\n        }\n      } finally {\n        this.creating = false;\n      }\n    },\n    viewFile(fileId) {\n      this.$router.push(`/file/${fileId}`);\n    },\n    confirmDelete(file) {\n      this.selectedFile = file;\n      this.showDeleteConfirm = true;\n    },\n    async deleteFile() {\n      if (!this.selectedFile || this.deleting) return;\n      this.deleting = true;\n      try {\n        await this.$axios.delete(`/file/${this.selectedFile.id}`);\n        await this.fetchFiles();\n        this.showDeleteConfirm = false;\n        this.selectedFile = null;\n      } catch (error) {\n        console.error('Âà†Èô§Êñá‰ª∂Â§±Ë¥•:', error);\n        if (error.response) {\n          alert(error.response.data.message || 'Âà†Èô§Êñá‰ª∂Â§±Ë¥•ÔºåÊúçÂä°Âô®ÈîôËØØ');\n        } else {\n          alert('Âà†Èô§Êñá‰ª∂Â§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï');\n        }\n      } finally {\n        this.deleting = false;\n      }\n    },\n    formatFileSize(file) {\n      const size = file.fileSize || 0;\n      if (size < 1024) {\n        return `${size} B`;\n      } else if (size < 1024 * 1024) {\n        return `${(size / 1024).toFixed(2)} KB`;\n      } else {\n        return `${(size / (1024 * 1024)).toFixed(2)} MB`;\n      }\n    },\n    formatDate(timestamp) {\n      if (!timestamp) return 'Êú™Áü•';\n      try {\n        const date = new Date(timestamp);\n        return date.toLocaleString('zh-CN', {\n          year: 'numeric',\n          month: '2-digit',\n          day: '2-digit',\n          hour: '2-digit',\n          minute: '2-digit',\n          second: '2-digit'\n        });\n      } catch (e) {\n        console.error('Êó•ÊúüÊ†ºÂºèÂåñÈîôËØØ:', e);\n        return 'Êó•ÊúüÊ†ºÂºèÈîôËØØ';\n      }\n    },\n    translatePermission(permission) {\n      const permissionMap = {\n        'read': 'Âè™ËØª',\n        'write': 'Âè™ÂÜô',\n        'readwrite': 'ËØªÂÜô'\n      };\n      return permissionMap[permission] || permission;\n    },\n    editFile(file) {\n      this.editingFile = {\n        id: file.id,\n        filename: file.filename,\n        content: file.content,\n        permission: file.permission\n      };\n      this.showEditModal = true;\n    },\n    cancelEdit() {\n      this.showEditModal = false;\n      this.editingFile = null;\n      this.editError = '';\n    },\n    async updateFile() {\n      if (!this.editingFile) return;\n      this.updating = true;\n      this.editError = '';\n      try {\n        // Êõ¥Êñ∞Êñá‰ª∂Âêç\n        await this.$axios.put(`/file/name/${this.editingFile.id}`, {\n          filename: this.editingFile.filename\n        });\n\n        // Êõ¥Êñ∞Êñá‰ª∂ÂÜÖÂÆπ\n        await this.$axios.put(`/file/content/${this.editingFile.id}`, {\n          content: this.editingFile.content\n        });\n\n        // Êõ¥Êñ∞Êñá‰ª∂ÊùÉÈôê\n        await this.$axios.put(`/file/permission/${this.editingFile.id}`, {\n          permission: this.editingFile.permission\n        });\n\n        // Âà∑Êñ∞Êñá‰ª∂ÂàóË°®\n        await this.fetchFiles();\n        this.showEditModal = false;\n        this.editingFile = null;\n      } catch (error) {\n        console.error('Êõ¥Êñ∞Êñá‰ª∂Â§±Ë¥•:', error);\n        if (error.response && error.response.data) {\n          this.editError = error.response.data.message;\n        } else {\n          this.editError = 'Êõ¥Êñ∞Êñá‰ª∂Â§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï';\n        }\n      } finally {\n        this.updating = false;\n      }\n    },\n    async openFile(file) {\n      this.opening = file.id;\n      try {\n        // Ëé∑ÂèñÊúÄÊñ∞ÁöÑÊñá‰ª∂ÂÜÖÂÆπ\n        const response = await this.$axios.get(`/file/${file.id}`);\n        if (response.data.success) {\n          this.openedFile = response.data.file;\n          this.openFiles.add(file.id);\n          localStorage.setItem('openFiles', JSON.stringify([...this.openFiles]));\n          const index = this.files.findIndex(f => f.id === file.id);\n          if (index !== -1) {\n            this.files[index].isOpen = true;\n          }\n        } else {\n          throw new Error(response.data.message || 'Ëé∑ÂèñÊñá‰ª∂ÂÜÖÂÆπÂ§±Ë¥•');\n        }\n      } catch (error) {\n        console.error('ÊâìÂºÄÊñá‰ª∂Â§±Ë¥•:', error);\n        alert(error.message || 'ÊâìÂºÄÊñá‰ª∂Â§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï');\n      } finally {\n        this.opening = null;\n      }\n    },\n    async closeFile(file) {\n      this.closing = file.id;\n      try {\n        this.openFiles.delete(file.id);\n        localStorage.setItem('openFiles', JSON.stringify([...this.openFiles]));\n        const index = this.files.findIndex(f => f.id === file.id);\n        if (index !== -1) {\n          this.files[index].isOpen = false;\n        }\n      } catch (error) {\n        console.error('ÂÖ≥Èó≠Êñá‰ª∂Â§±Ë¥•:', error);\n        alert('ÂÖ≥Èó≠Êñá‰ª∂Â§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï');\n        this.openFiles.add(file.id);\n      } finally {\n        this.closing = null;\n      }\n    },\n    // ÂÖ≥Èó≠ÊâìÂºÄÁöÑÊñá‰ª∂\n    closeOpenedFile() {\n      if (this.openedFile) {\n        const file = this.files.find(f => f.id === this.openedFile.id);\n        if (file) {\n          this.closeFile(file);\n        }\n        this.openedFile = null;\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","loading","files","error","userId","localStorage","getItem","newFile","filename","content","permission","creating","createError","createSuccess","showDeleteConfirm","selectedFile","deleting","showEditModal","editingFile","editError","updating","openFiles","Set","JSON","parse","opening","closing","openedFile","created","fetchFiles","methods","response","$axios","get","success","map","file","isOpen","has","id","console","message","request","createFile","post","parseInt","viewFile","fileId","$router","push","confirmDelete","deleteFile","delete","alert","formatFileSize","size","fileSize","toFixed","formatDate","timestamp","date","Date","toLocaleString","year","month","day","hour","minute","second","e","translatePermission","permissionMap","editFile","cancelEdit","updateFile","put","openFile","add","setItem","stringify","index","findIndex","f","Error","closeFile","closeOpenedFile","find"],"sources":["src/components/FileList.vue"],"sourcesContent":["<template>\r\n  <div class=\"file-list\">\r\n    <!-- È°µÈù¢Ê†áÈ¢òÂíåÁªüËÆ°‰ø°ÊÅØ -->\r\n    <div class=\"page-header\">\r\n      <div class=\"header-content\">\r\n        <h2>ÊàëÁöÑÊñá‰ª∂</h2>\r\n        <div class=\"file-stats\">\r\n          <div class=\"stat-item\">\r\n            <span class=\"stat-icon\">üìÑ</span>\r\n            <span class=\"stat-value\">{{ files.length }}</span>\r\n            <span class=\"stat-label\">‰∏™Êñá‰ª∂</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- ÂàõÂª∫Êñ∞Êñá‰ª∂Ë°®Âçï -->\r\n    <div class=\"card create-file-card\">\r\n      <div class=\"card-header\">\r\n        <h3>\r\n          <span class=\"card-icon\">‚ú®</span>\r\n          ÂàõÂª∫Êñ∞Êñá‰ª∂\r\n        </h3>\r\n      </div>\r\n      \r\n      <form @submit.prevent=\"createFile\">\r\n        <div class=\"form-grid\">\r\n          <div class=\"form-group\">\r\n            <label for=\"filename\">\r\n              <span class=\"label-icon\">üìù</span>\r\n              Êñá‰ª∂Âêç\r\n            </label>\r\n            <input \r\n              id=\"filename\" \r\n              v-model=\"newFile.filename\" \r\n              type=\"text\" \r\n              placeholder=\"ËØ∑ËæìÂÖ•Êñá‰ª∂Âêç\" \r\n              required\r\n            />\r\n          </div>\r\n          \r\n          <div class=\"form-group\">\r\n            <label for=\"permission\">\r\n              <span class=\"label-icon\">üîí</span>\r\n              Êñá‰ª∂ÊùÉÈôê\r\n            </label>\r\n            <select id=\"permission\" v-model=\"newFile.permission\" required>\r\n              <option value=\"read\">Âè™ËØª</option>\r\n              <option value=\"write\">Âè™ÂÜô</option>\r\n              <option value=\"readwrite\">ËØªÂÜô</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"form-group\">\r\n          <label for=\"content\">\r\n            <span class=\"label-icon\">üìÑ</span>\r\n            Êñá‰ª∂ÂÜÖÂÆπ\r\n          </label>\r\n          <textarea \r\n            id=\"content\" \r\n            v-model=\"newFile.content\" \r\n            rows=\"4\" \r\n            placeholder=\"ËØ∑ËæìÂÖ•Êñá‰ª∂ÂÜÖÂÆπ\"\r\n          ></textarea>\r\n        </div>\r\n        \r\n        <div class=\"error\" v-if=\"createError\">{{ createError }}</div>\r\n        <div class=\"success\" v-if=\"createSuccess\">{{ createSuccess }}</div>\r\n        \r\n        <button type=\"submit\" :disabled=\"creating\" class=\"create-btn\">\r\n          <span class=\"btn-content\">\r\n            <span v-if=\"creating\" class=\"loading-spinner\"></span>\r\n            {{ creating ? 'ÂàõÂª∫‰∏≠...' : 'ÂàõÂª∫Êñá‰ª∂' }}\r\n          </span>\r\n        </button>\r\n      </form>\r\n    </div>\r\n    \r\n    <!-- Êñá‰ª∂ÂàóË°® -->\r\n    <div class=\"card file-table-card\">\r\n      <div class=\"card-header\">\r\n        <h3>\r\n          <span class=\"card-icon\">üìã</span>\r\n          Êñá‰ª∂ÂàóË°®\r\n        </h3>\r\n      </div>\r\n      \r\n      <div v-if=\"loading\" class=\"loading-state\">\r\n        <div class=\"loading-spinner\"></div>\r\n        <span>Âä†ËΩΩ‰∏≠...</span>\r\n      </div>\r\n      \r\n      <div v-else-if=\"error\" class=\"error-state\">\r\n        <span class=\"error-icon\">‚ö†Ô∏è</span>\r\n        <p>{{ error }}</p>\r\n        <button @click=\"fetchFiles\" class=\"retry-btn\">ÈáçËØï</button>\r\n      </div>\r\n      \r\n      <div v-else-if=\"files.length === 0\" class=\"empty-state\">\r\n        <span class=\"empty-icon\">üì≠</span>\r\n        <p>ÊÇ®ËøòÊ≤°ÊúâÂàõÂª∫‰ªª‰ΩïÊñá‰ª∂</p>\r\n      </div>\r\n      \r\n      <div v-else class=\"table-container\">\r\n        <table>\r\n          <thead>\r\n            <tr>\r\n              <th>Êñá‰ª∂Âêç</th>\r\n              <th>Â§ßÂ∞è</th>\r\n              <th>ÊùÉÈôê</th>\r\n              <th>ÂàõÂª∫Êó∂Èó¥</th>\r\n              <th>Êìç‰Ωú</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"file in files\" :key=\"file.id\">\r\n              <td class=\"filename-cell\">\r\n                <span class=\"file-icon\">üìÑ</span>\r\n                {{ file.filename }}\r\n              </td>\r\n              <td>{{ formatFileSize(file) }}</td>\r\n              <td>\r\n                <span class=\"permission-tag\" :class=\"file.permission\">\r\n                  {{ translatePermission(file.permission) }}\r\n                </span>\r\n              </td>\r\n              <td>{{ formatDate(file.createTime) }}</td>\r\n              <td class=\"actions\">\r\n                <button @click=\"viewFile(file.id)\" class=\"action-btn view-btn\" title=\"Êü•Áúã\">\r\n                  üëÅÔ∏è\r\n                </button>\r\n                <button @click=\"editFile(file)\" class=\"action-btn edit-btn\" title=\"‰øÆÊîπ\">\r\n                  ‚úèÔ∏è\r\n                </button>\r\n                <button \r\n                  v-if=\"!file.isOpen\" \r\n                  @click=\"openFile(file)\" \r\n                  class=\"action-btn open-btn\"\r\n                  :disabled=\"opening === file.id\"\r\n                  title=\"ÊâìÂºÄ\"\r\n                >\r\n                  üìÇ\r\n                </button>\r\n                <button \r\n                  v-else \r\n                  @click=\"closeFile(file)\" \r\n                  class=\"action-btn close-btn\"\r\n                  :disabled=\"closing === file.id\"\r\n                  title=\"ÂÖ≥Èó≠\"\r\n                >\r\n                  üìÅ\r\n                </button>\r\n                <button @click=\"confirmDelete(file)\" class=\"action-btn delete-btn\" title=\"Âà†Èô§\">\r\n                  üóëÔ∏è\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- Âà†Èô§Á°ÆËÆ§ÂØπËØùÊ°Ü -->\r\n    <transition name=\"fade\">\r\n      <div v-if=\"showDeleteConfirm\" class=\"delete-modal\">\r\n        <div class=\"delete-modal-content card\">\r\n          <h3>Á°ÆËÆ§Âà†Èô§</h3>\r\n          <p>ÊÇ®Á°ÆÂÆöË¶ÅÂà†Èô§Êñá‰ª∂ <strong>{{ selectedFile.filename }}</strong> ÂêóÔºü</p>\r\n          <p class=\"warning\">Ê≠§Êìç‰Ωú‰∏çÂèØÊí§ÈîÄÔºÅ</p>\r\n          <div class=\"modal-actions\">\r\n            <button @click=\"deleteFile\" class=\"confirm-delete\">Á°ÆËÆ§Âà†Èô§</button>\r\n            <button @click=\"showDeleteConfirm = false\" class=\"cancel-delete\">ÂèñÊ∂à</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n    \r\n    <!-- Ê∑ªÂä†‰øÆÊîπÊñá‰ª∂ÁöÑÊ®°ÊÄÅÊ°Ü -->\r\n    <div v-if=\"showEditModal\" class=\"modal\">\r\n      <div class=\"modal-content card\">\r\n        <h3>‰øÆÊîπÊñá‰ª∂</h3>\r\n        <form @submit.prevent=\"updateFile\">\r\n          <div class=\"form-group\">\r\n            <label for=\"edit-filename\">Êñá‰ª∂Âêç</label>\r\n            <input \r\n              id=\"edit-filename\" \r\n              v-model=\"editingFile.filename\" \r\n              type=\"text\" \r\n              required\r\n            />\r\n          </div>\r\n          \r\n          <div class=\"form-group\">\r\n            <label for=\"edit-content\">Êñá‰ª∂ÂÜÖÂÆπ</label>\r\n            <textarea \r\n              id=\"edit-content\" \r\n              v-model=\"editingFile.content\" \r\n              rows=\"6\"\r\n            ></textarea>\r\n          </div>\r\n          \r\n          <div class=\"form-group\">\r\n            <label for=\"edit-permission\">Êñá‰ª∂ÊùÉÈôê</label>\r\n            <select id=\"edit-permission\" v-model=\"editingFile.permission\">\r\n              <option value=\"read\">Âè™ËØª</option>\r\n              <option value=\"write\">Âè™ÂÜô</option>\r\n              <option value=\"readwrite\">ËØªÂÜô</option>\r\n            </select>\r\n          </div>\r\n          \r\n          <div class=\"error\" v-if=\"editError\">{{ editError }}</div>\r\n          \r\n          <div class=\"modal-actions\">\r\n            <button type=\"submit\" :disabled=\"updating\">\r\n              {{ updating ? '‰øùÂ≠ò‰∏≠...' : '‰øùÂ≠ò‰øÆÊîπ' }}\r\n            </button>\r\n            <button type=\"button\" @click=\"cancelEdit\" class=\"cancel-btn\">ÂèñÊ∂à</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- Ê∑ªÂä†ÊâìÂºÄÊñá‰ª∂ÁöÑÊ®°ÊÄÅÊ°Ü -->\r\n    <transition name=\"fade\">\r\n      <div v-if=\"openedFile\" class=\"file-modal\">\r\n        <div class=\"file-modal-content card\">\r\n          <div class=\"file-modal-header\">\r\n            <h3>{{ openedFile.filename }}</h3>\r\n            <button class=\"close-modal-btn\" @click=\"closeOpenedFile\">√ó</button>\r\n          </div>\r\n          \r\n          <div class=\"file-info-bar\">\r\n            <span>Â§ßÂ∞è: {{ formatFileSize(openedFile) }}</span>\r\n            <span>ÊùÉÈôê: {{ translatePermission(openedFile.permission) }}</span>\r\n            <span>ÂàõÂª∫Êó∂Èó¥: {{ formatDate(openedFile.createTime) }}</span>\r\n          </div>\r\n          \r\n          <div class=\"file-content-viewer\">\r\n            <pre>{{ openedFile.content }}</pre>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'FileList',\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      files: [],\r\n      error: '',\r\n      userId: localStorage.getItem('userId'),\r\n      \r\n      // Êñ∞Êñá‰ª∂Ë°®Âçï\r\n      newFile: {\r\n        filename: '',\r\n        content: '',\r\n        permission: 'read'\r\n      },\r\n      creating: false,\r\n      createError: '',\r\n      createSuccess: '',\r\n      \r\n      // Âà†Èô§Á°ÆËÆ§\r\n      showDeleteConfirm: false,\r\n      selectedFile: null,\r\n      deleting: false,\r\n      \r\n      // Ê∑ªÂä†ÁºñËæëÁõ∏ÂÖ≥ÁöÑÊï∞ÊçÆ\r\n      showEditModal: false,\r\n      editingFile: null,\r\n      editError: '',\r\n      updating: false,\r\n      \r\n      // ËÆ∞ÂΩïÊâìÂºÄÁöÑÊñá‰ª∂IDÂàóË°®\r\n      openFiles: new Set(JSON.parse(localStorage.getItem('openFiles') || '[]')),\r\n      opening: null,\r\n      closing: null,\r\n      openedFile: null, // ÂΩìÂâçÊâìÂºÄÁöÑÊñá‰ª∂\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchFiles();\r\n  },\r\n  methods: {\r\n    async fetchFiles() {\r\n      this.loading = true;\r\n      this.error = '';\r\n      \r\n      try {\r\n        const userId = localStorage.getItem('userId');\r\n        const response = await this.$axios.get(`/file/list/${userId}`);\r\n        \r\n        if (response.data.success) {\r\n          // ‰∏∫ÊØè‰∏™Êñá‰ª∂Ê∑ªÂä†ÊâìÂºÄÁä∂ÊÄÅ\r\n          this.files = (response.data.files || []).map(file => ({\r\n            ...file,\r\n            isOpen: this.openFiles.has(file.id)\r\n          }));\r\n        } else {\r\n          this.error = 'Ëé∑ÂèñÊñá‰ª∂ÂàóË°®Â§±Ë¥•';\r\n        }\r\n      } catch (error) {\r\n        console.error('Ëé∑ÂèñÊñá‰ª∂ÂàóË°®Â§±Ë¥•:', error);\r\n        if (error.response) {\r\n          console.error('ÈîôËØØÂìçÂ∫î:', error.response.data);\r\n          this.error = error.response.data.message || 'Ëé∑ÂèñÊñá‰ª∂ÂàóË°®Â§±Ë¥•ÔºåÊúçÂä°Âô®ÈîôËØØ';\r\n        } else if (error.request) {\r\n          this.error = 'Êó†Ê≥ïËøûÊé•Âà∞ÊúçÂä°Âô®ÔºåËØ∑Ê£ÄÊü•ÁΩëÁªúËøûÊé•';\r\n        } else {\r\n          this.error = 'Ëé∑ÂèñÊñá‰ª∂ÂàóË°®Â§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï';\r\n        }\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n    \r\n    async createFile() {\r\n      this.creating = true;\r\n      this.createError = '';\r\n      this.createSuccess = '';\r\n      \r\n      try {\r\n        const userId = localStorage.getItem('userId');\r\n        await this.$axios.post('/file/create', {\r\n          userId: parseInt(userId),\r\n          filename: this.newFile.filename,\r\n          content: this.newFile.content || '',\r\n          permission: this.newFile.permission\r\n        });\r\n        \r\n        this.createSuccess = 'Êñá‰ª∂ÂàõÂª∫ÊàêÂäüÔºÅ';\r\n        // ÈáçÁΩÆË°®Âçï\r\n        this.newFile = {\r\n          filename: '',\r\n          content: '',\r\n          permission: 'read'\r\n        };\r\n        // Âà∑Êñ∞Êñá‰ª∂ÂàóË°®\r\n        await this.fetchFiles();\r\n      } catch (error) {\r\n        console.error('ÂàõÂª∫Êñá‰ª∂Â§±Ë¥•:', error);\r\n        if (error.response) {\r\n          this.createError = error.response.data.message || 'ÂàõÂª∫Êñá‰ª∂Â§±Ë¥•ÔºåÊúçÂä°Âô®ÈîôËØØ';\r\n        } else if (error.request) {\r\n          this.createError = 'Êó†Ê≥ïËøûÊé•Âà∞ÊúçÂä°Âô®ÔºåËØ∑Ê£ÄÊü•ÁΩëÁªúËøûÊé•';\r\n        } else {\r\n          this.createError = 'ÂàõÂª∫Êñá‰ª∂Â§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï';\r\n        }\r\n      } finally {\r\n        this.creating = false;\r\n      }\r\n    },\r\n    \r\n    viewFile(fileId) {\r\n      this.$router.push(`/file/${fileId}`);\r\n    },\r\n    \r\n    confirmDelete(file) {\r\n      this.selectedFile = file;\r\n      this.showDeleteConfirm = true;\r\n    },\r\n    \r\n    async deleteFile() {\r\n      if (!this.selectedFile || this.deleting) return;\r\n      \r\n      this.deleting = true;\r\n      try {\r\n        await this.$axios.delete(`/file/${this.selectedFile.id}`);\r\n        await this.fetchFiles();\r\n        this.showDeleteConfirm = false;\r\n        this.selectedFile = null;\r\n      } catch (error) {\r\n        console.error('Âà†Èô§Êñá‰ª∂Â§±Ë¥•:', error);\r\n        if (error.response) {\r\n          alert(error.response.data.message || 'Âà†Èô§Êñá‰ª∂Â§±Ë¥•ÔºåÊúçÂä°Âô®ÈîôËØØ');\r\n        } else {\r\n          alert('Âà†Èô§Êñá‰ª∂Â§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï');\r\n        }\r\n      } finally {\r\n        this.deleting = false;\r\n      }\r\n    },\r\n    \r\n    formatFileSize(file) {\r\n      const size = file.fileSize || 0;\r\n      if (size < 1024) {\r\n        return `${size} B`;\r\n      } else if (size < 1024 * 1024) {\r\n        return `${(size / 1024).toFixed(2)} KB`;\r\n      } else {\r\n        return `${(size / (1024 * 1024)).toFixed(2)} MB`;\r\n      }\r\n    },\r\n    \r\n    formatDate(timestamp) {\r\n      if (!timestamp) return 'Êú™Áü•';\r\n      try {\r\n        const date = new Date(timestamp);\r\n        return date.toLocaleString('zh-CN', {\r\n          year: 'numeric',\r\n          month: '2-digit',\r\n          day: '2-digit',\r\n          hour: '2-digit',\r\n          minute: '2-digit',\r\n          second: '2-digit'\r\n        });\r\n      } catch (e) {\r\n        console.error('Êó•ÊúüÊ†ºÂºèÂåñÈîôËØØ:', e);\r\n        return 'Êó•ÊúüÊ†ºÂºèÈîôËØØ';\r\n      }\r\n    },\r\n    \r\n    translatePermission(permission) {\r\n      const permissionMap = {\r\n        'read': 'Âè™ËØª',\r\n        'write': 'Âè™ÂÜô',\r\n        'readwrite': 'ËØªÂÜô'\r\n      };\r\n      return permissionMap[permission] || permission;\r\n    },\r\n    \r\n    editFile(file) {\r\n      this.editingFile = {\r\n        id: file.id,\r\n        filename: file.filename,\r\n        content: file.content,\r\n        permission: file.permission\r\n      };\r\n      this.showEditModal = true;\r\n    },\r\n    \r\n    cancelEdit() {\r\n      this.showEditModal = false;\r\n      this.editingFile = null;\r\n      this.editError = '';\r\n    },\r\n    \r\n    async updateFile() {\r\n      if (!this.editingFile) return;\r\n      \r\n      this.updating = true;\r\n      this.editError = '';\r\n      \r\n      try {\r\n        // Êõ¥Êñ∞Êñá‰ª∂Âêç\r\n        await this.$axios.put(`/file/name/${this.editingFile.id}`, {\r\n          filename: this.editingFile.filename\r\n        });\r\n        \r\n        // Êõ¥Êñ∞Êñá‰ª∂ÂÜÖÂÆπ\r\n        await this.$axios.put(`/file/content/${this.editingFile.id}`, {\r\n          content: this.editingFile.content\r\n        });\r\n        \r\n        // Êõ¥Êñ∞Êñá‰ª∂ÊùÉÈôê\r\n        await this.$axios.put(`/file/permission/${this.editingFile.id}`, {\r\n          permission: this.editingFile.permission\r\n        });\r\n        \r\n        // Âà∑Êñ∞Êñá‰ª∂ÂàóË°®\r\n        await this.fetchFiles();\r\n        this.showEditModal = false;\r\n        this.editingFile = null;\r\n      } catch (error) {\r\n        console.error('Êõ¥Êñ∞Êñá‰ª∂Â§±Ë¥•:', error);\r\n        if (error.response && error.response.data) {\r\n          this.editError = error.response.data.message;\r\n        } else {\r\n          this.editError = 'Êõ¥Êñ∞Êñá‰ª∂Â§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï';\r\n        }\r\n      } finally {\r\n        this.updating = false;\r\n      }\r\n    },\r\n    \r\n    async openFile(file) {\r\n      this.opening = file.id;\r\n      try {\r\n        // Ëé∑ÂèñÊúÄÊñ∞ÁöÑÊñá‰ª∂ÂÜÖÂÆπ\r\n        const response = await this.$axios.get(`/file/${file.id}`);\r\n        if (response.data.success) {\r\n          this.openedFile = response.data.file;\r\n          this.openFiles.add(file.id);\r\n          localStorage.setItem('openFiles', JSON.stringify([...this.openFiles]));\r\n          const index = this.files.findIndex(f => f.id === file.id);\r\n          if (index !== -1) {\r\n            this.files[index].isOpen = true;\r\n          }\r\n        } else {\r\n          throw new Error(response.data.message || 'Ëé∑ÂèñÊñá‰ª∂ÂÜÖÂÆπÂ§±Ë¥•');\r\n        }\r\n      } catch (error) {\r\n        console.error('ÊâìÂºÄÊñá‰ª∂Â§±Ë¥•:', error);\r\n        alert(error.message || 'ÊâìÂºÄÊñá‰ª∂Â§±Ë¥•ÔºåËØ∑Á®çÂêéÂÜçËØï');\r\n      } finally {\r\n        this.opening = null;\r\n      }\r\n    },\r\n    \r\n    async closeFile(file) {\r\n      this.closing = file.id;\r\n      try {\r\n        this.openFiles.delete(file.id);\r\n        localStorage.setItem('openFiles', JSON.stringify([...this.openFiles]));\r\n        const index = this.files.findIndex(f => f.id === file.id);\r\n        if (index !== -1) {\r\n          this.files[index].isOpen = false;\r\n        }\r\n      } catch (error) {\r\n       